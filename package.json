{
  "name": "postcss-split-common",
  "version": "0.0.0",
  "description": "PostCSS plugin to split common rules at top and bottom",
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "postcss-split-common"
  ],
  "scripts": {
    "test": "jest --coverage && eslint ."
  },
  "author": "Srikanth Sharma <snow.650@gmail.com>",
  "license": "MIT",
  "repository": "itzsrikanth/postcss-split-common",
  "engines": {
    "node": ">=12.0.0"
  },
  "peerDependencies": {
    "postcss": "^8.3.0"
  },
  "devDependencies": {
    "clean-publish": "^3.4.2",
    "eslint": "^8.0.1",
    "eslint-plugin-jest": "^25.2.2",
    "jest": "^27.3.1",
    "postcss": "^8.3.11"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2017
    },
    "env": {
      "node": true,
      "es6": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:jest/recommended"
    ],
    "rules": {
      "jest/expect-expect": "off"
    }
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "statements": 100
      }
    }
  },
  "volta": {
    "node": "16.19.1",
    "yarn": "1.22.19"
  },
  "dependencies": {
    "walk-sync": "^3.0.0"
  }
}
